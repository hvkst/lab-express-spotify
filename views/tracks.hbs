<h1 id='ArtistHeading'>{{nameOfAlbum}}</h1>

{{#each trackArray}}

  <div id='audio-wrapper'>
    <figure>
      <figcaption>{{this.name}}</figcaption>
      <audio controls src='{{this.preview_url}}'>
        Your browser does not support the
        <code>audio</code>
        element.
      </audio>
    </figure>
  </div>

{{/each}}